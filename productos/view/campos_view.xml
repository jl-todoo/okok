
<odoo>
	<data>	
		<record model="ir.ui.view" id="res_partner_form_view_inh">
		      <field name="name">res.partner.form.view.inh</field>
		      <field name="model">res.partner</field>
		      <field name="type">form</field>
		      <field name="inherit_id" ref="carpetapadre.idVistaPadre" />
		      <field name="arch" type="xml">
		      	<xpath expr="//field[@name='barcode']" position="after">
            		<field name="clase_impres"/>
            		<field name="presentacion"/>
            		<field name="tipo_selle"/>
        		</xpath>
        		<xpath expr="//field[@name='list_price']" position="before">
        		 	<field name="referencia"/>
        		</xpath>	
        		<xpath expr="//field[@name='list_price']" position="after">
        		 	<field name="moneda"/>
        		</xpath>	
        		<xpath expr="//field[@name='standard_price']" position="after">
        		 	<field name="version"/>
        		 	<field name="fecha_version"/>
        		</xpath>
			</field>
		</record>
	</data>
</odoo>